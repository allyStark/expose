{
  "manifest_version": 2,
  
  "name": "eXpose",
  "author": "roboticsound",
  "description": "Publish WebExtensions with one click.",
  "version": "0.1",

  "background": {
      "page": "background.html"
  },

  "content_scripts": [
      {
      "matches": ["https://github.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
      }
  ],

  "options_ui": {
    "page": "/options.html",
    "chrome_style": false
  },

  "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self' ",

  "permissions": [
    "identity",
    "activeTab",
    "downloads",
    "webNavigation",
    "storage",
    "https://ajax.googleapis.com/",
    "<all_urls>"
  ],
